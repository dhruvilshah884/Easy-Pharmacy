<form method="post">
    <table>
        <tr>
            <td>Name: </td>
            <td>
                <input type="text" placeholder="Enter name " name="user_name" value="<%= db_rows_array[0].user_name %>"
                    required>
            </td>

        </tr>
        <tr>
            <td>Gender:</td>
            <td>
                <input type="text" placeholder="Enter Gender " name="user_gender"
                    value="<%= db_rows_array[0].user_gender %>" required>
            </td>
        </tr>
        <tr>
            <td>Email: </td>
            <td><input type="email" placeholder="Enter email" name="user_email"
                    value="<%= db_rows_array[0].user_email %>" required>
            </td>
        </tr>
        <tr>
            <td>Password:</td>
            <td>
                <input type="text" placeholder="Enter Password" name="user_password"
                    value="<%= db_rows_array[0].user_password %>" required>
            </td>
        </tr>
        <tr>
            <td>mobile:</td>
            <td>
                <input type="tel" placeholder="Enter mobile" name="user_mobile"
                    value="<%= db_rows_array[0].user_mobile %>" required>
            </td>
        </tr>
        <tr>
            <td>
                <input type="submit">
                <input type="button" value="user_view" onclick=" window.location='user_view' ">
            </td>
        </tr>
    </table>

</form>